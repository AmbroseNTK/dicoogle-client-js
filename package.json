{
  "name": "dicoogle-client",
  "author": "Universidade de Aveiro, DETI/IEETA, Bioinformatics Group (http://bioinformatics.ua.pt/)",
  "maintainers": [
    "Eduardo Pinho <eduardopinho@ua.pt>"
  ],
  "contributors": [
    "Eduardo Pinho <eduardopinho@ua.pt>",
    "Luís Bastião Silva <bastiao@bmd-software.com>"
  ],
  "description": "Dicoogle client API for JavaScript applications",
  "keywords": [
    "dicoogle",
    "client",
    "api"
  ],
  "version": "4.2.0",
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "./lib/index",
  "types": "./types/index.d.ts",
  "browserify": {
    "transforms": [
      "envify"
    ]
  },
  "files": [
    "dist/",
    "bin/",
    "lib/"
  ],
  "bin": {
    "dicoogle-query": "bin/dicoogle-query-cli.js"
  },
  "license": "GPL-3.0+",
  "dependencies": {
    "@types/superagent": "^4.1.3",
    "superagent": "^5.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "browserify": "^16.5.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.6",
    "cross-env": "^6.0.0",
    "envify": "^4.0.0",
    "gulp": "^4.0.2",
    "gulp-eslint": "^6.0.0",
    "gulp-header": "^2.0.9",
    "gulp-rename": "^1.2.2",
    "gulp-rm": "^2.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typescript": "^5.0.1",
    "gulp-uglify": "^3.0.0",
    "mocha": "^6.2.0",
    "nock": "^11.3.1",
    "nyc": "^14.1.1",
    "ts-node": "^8.4.1",
    "typedoc": "^0.14.2",
    "typescript": "^3.6.3",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "scripts": {
    "check": "gulp --series lint",
    "build": "gulp --series lint main bundle",
    "prepare": "gulp default",
    "clean": "gulp clean",
    "document": "cd types && typedoc --includeDeclarations --out ../docs/ --target ES6 --name 'Dicoogle JavaScript Client' --readme ../README.docs.md index.d.ts",
    "test": "cross-env NODE_ENV=test mocha -r ts-node/register",
    "cover": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha -r ts-node/register",
    "coveralls": "cross-env NODE_ENV=test nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bioinformatics-ua/dicoogle-client-js.git"
  },
  "nyc": {
    "extension": [
      ".js",
      ".ts"
    ],
    "exclude": [
      "gulpfile.js",
      "test/*"
    ]
  }
}
