{
  "name": "dicoogle-client",
  "author": "Universidade de Aveiro, DETI/IEETA, Bioinformatics Group (http://bioinformatics.ua.pt/)",
  "maintainers": [
    "Eduardo Pinho <eduardopinho@ua.pt>"
  ],
  "contributors": [
    "Eduardo Pinho <eduardopinho@ua.pt>",
    "Luís Bastião Silva <bastiao@bmd-software.com>"
  ],
  "description": "Dicoogle client API for JavaScript applications",
  "keywords": [
    "dicoogle",
    "client",
    "api"
  ],
  "version": "4.0.0",
  "engines": {
    "node": ">=4.0.0"
  },
  "main": "./lib/index",
  "types": "./types/dicoogle-client.d.ts",
  "browserify": {
    "transforms": [
      "envify"
    ]
  },
  "files": [
    "dist/",
    "bin/",
    "lib/",
    "types/"
  ],
  "bin": {
    "dicoogle-query": "bin/dicoogle-query-cli.js"
  },
  "license": "GPL-3.0+",
  "dependencies": {
    "@types/superagent": "^2.0.36",
    "superagent": "^3.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.0",
    "babel-plugin-istanbul": "^3",
    "babel-preset-es2015": "^6.1.2",
    "babel-register": "^6.14.0",
    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "cross-env": "^3.1.4",
    "envify": "^4.0.0",
    "estraverse": "^4.1.1",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.1",
    "gulp-eslint": "3.0.1",
    "gulp-header": "^1.7.1",
    "gulp-rename": "^1.2.2",
    "gulp-rm": "^1.0.0",
    "gulp-uglify": "^2",
    "mocha": "^3.2.0",
    "nock": "^9.0.2",
    "nyc": "^10.0.0",
    "typedoc": "^0.5.3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "scripts": {
    "check": "gulp lint",
    "build": "gulp",
    "prepublish": "gulp",
    "document": "typedoc --out doc/ --includeDeclarations --target ES6 --name 'Dicoogle JavaScript Client' --readme README.md types/dicoogle-client.d.ts",
    "test": "cross-env NODE_ENV=test nyc mocha",
    "cover": "cross-env NODE_ENV=test nyc report --reporter=lcov",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bioinformatics-ua/dicoogle-client-js.git"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
