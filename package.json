{
  "name": "dicoogle-client",
  "author": "Universidade de Aveiro, DETI/IEETA, Bioinformatics Group (http://bioinformatics.ua.pt/)",
  "maintainers": [
    "Eduardo Pinho <eduardopinho@ua.pt>"
  ],
  "contributors": [
    "Eduardo Pinho <eduardopinho@ua.pt>",
    "Luís Bastião Silva <bastiao@bmd-software.com>"
  ],
  "description": "Dicoogle client API for JavaScript applications",
  "keywords": [
    "dicoogle",
    "client",
    "api"
  ],
  "version": "3.4.0",
  "engines": {
    "node": ">=0.12.0"
  },
  "main": "./lib/index",
  "typings": "./typings/dicoogle-client.d.ts",
  "browserify": {
    "transforms": [
      "envify"
    ]
  },
  "files": [
    "dist/",
    "bin/",
    "lib/"
  ],
  "bin": {
    "dicoogle-query": "bin/dicoogle-query-cli.js"
  },
  "license": "GPL-3.0+",
  "dependencies": {
    "superagent": "^1.7.2"
  },
  "devDependencies": {
    "babel-plugin-add-module-exports": "^0.1.4",
    "babel-preset-es2015": "^6.1.2",
    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "envify": "^3.4.0",
    "estraverse": "^4.1.1",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.1",
    "gulp-eslint": "^2.0.0",
    "gulp-header": "^1.7.1",
    "gulp-rename": "^1.2.2",
    "gulp-rm": "^1.0.0",
    "gulp-sourcemaps": "^2.0.0-alpha",
    "gulp-uglify": "^1.4.2",
    "istanbul": "^0.4.3",
    "mocha": "^2.3.4",
    "mocha-lcov-reporter": "^1.2.0",
    "nock": "^8.0.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "scripts": {
    "check": "gulp lint",
    "build": "gulp",
    "prepublish": "gulp",
    "test": "mocha",
    "cover": "istanbul cover _mocha --report lcovonly -- -R spec",
    "coveralls": "npm run cover && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bioinformatics-ua/dicoogle-client-js.git"
  }
}
